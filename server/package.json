{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/app.js",
    "start:dev": "nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm -- --dataSource=./src/database/app.data-source.ts migration:generate",
    "migration:run": "npm run typeorm -- --dataSource=./src/database/app.data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=./src/database/app.data-source.ts migration:revert",
    "seed:orders": "ts-node -r tsconfig-paths/register src/database/seed/orders.seed.ts",
    "trigger": "ts-node ./src/database/triggers/index.ts",
    "seed:orders:massive": "ts-node -r tsconfig-paths/register src/database/seed/orders-massive.seed.ts"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "install": "^0.13.0",
    "nodemon": "^3.1.11",
    "npm": "^11.6.4",
    "prettier": "^3.7.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bullmq": "^5.65.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.27"
  }
}
